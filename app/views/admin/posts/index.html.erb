<h1>Blog</h1>

<%= link_to 'Create new post', new_admin_post_path %>
<br /><br />
<% @posts.each do |post| %>
  <%= post.created_at %>
  <br />
  <h2><%= post.title %></h2>
  <%= link_to 'Show', [:admin, post] %> |
  <%= link_to 'Edit', edit_admin_post_path(post) %> |
  <%= link_to 'Destroy', [:admin, post], method: :delete, data: { confirm: 'Are you sure?' } %>
  <br /><br />
<% end %>
<%= will_paginate @posts %>


